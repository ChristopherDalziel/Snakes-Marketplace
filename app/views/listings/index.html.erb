<h1>Listings List</h1>

<table>
  <thead>
    <tr>
      <th>Id:</th>
      <th>Title:</th>
      <th>Breed ID:</th>
      <th>Description:</th>
      <th>Sex:</th>
      <th>Price:</th>
      <th>Deposit:</th>
      <th>City:</th>
      <th>State:</th>
      <th>Date of Birth:</th>
      <th>Diet:</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.id %></td>
        <td><%= listing.title %></td>
        <td><%= listing.breed.name %></td>
        <td><%= listing.description %></td>
        <td><%= listing.sex %></td>
        <td><%= listing.price %></td>
        <td><%= listing.deposit %></td>
        <td><%= listing.city %></td>
        <td><%= listing.state %></td>
        <td><%= listing.date_of_birth %></td>
        <td><%= listing.diet %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Show', listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing_path(listing), method: :delete, data: { confirm: 'Are you sure?' }%></td>

        
  
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_listing_path %>
<%= link_to 'Home', root_path %>